#ulibasn1 pkg-config source file

prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: ulibasn1
Cflags: -I${includedir}
Libs: -L${libdir} -l:libulibasn1.@SHARED_LIBRARY_EXTENSION@.@MAJORVER@.@MINORVER@

Description: universal ASN1 library
Version: @VERSION@
Requires: ulib 


